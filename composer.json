{
  "name": "magevishwas/magento2-helloworld-extension",
  "description": "A basic Magento 2 module.",
  "type": "magento2-module",
  "version": "1.0.0",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "require": {
    "php": "~5.5.0|~5.6.0",
    "magento/magento-composer-installer": "*",
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.magento.com/"
    }
    {
      "type": "vcs",
      "url": "https://github.com/magevishwas/magento2-helloworld-extension"
    }
  ],
  "extra": {
    "map": [
      [
        "*",
        "Vishwas/Hello"
      ]
    ]
  },

  "minimum-stability": "dev",
  "prefer-stable": true,
}